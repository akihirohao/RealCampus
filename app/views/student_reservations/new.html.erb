<div class="shift_form">

  <div class="shift_form_title"><登録フォーム></div>
    <%= form_for @reservation, url: student_reservations_path  do |f| %>
       <div class="shift_form_shorttitle">
        <p>案内したい時間を登録しよ！</p>
       </div>
        <%= f.date_field :time %>
        <%= f.hidden_field :status, value: 0%>
        <%= f.submit %>

    <% end %>

  <div class="shift_form_image">
  </div>

</div>